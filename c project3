Nested condition in c #35

#include <stdio.h>
#include<stdbool.h>
int main(void){
int age =19;
bool has_nid = true;
bool has_passport = true;
if(age>=18){
if(has_nid||has_passport){
printf("Ok,Lets proceed\n");
}else{
printf("You are not accepted\n");
}
}else{
printf("You are not allowed\n");
}

return 0;
}
