else if condition-2 #32

#include <stdio.h>
int main(void){
char color_code;
printf("Enter color code:");
scanf("%c",& color_code);
if(color_code =='R'){
printf("It is Red\n");
} else if(color_code =='G'){
printf("It is Green\n");
}
else if(color_code =='B'){
printf("It is Blue\n");
}
else if(color_code =='Y'){
printf("It is Yellow\n");
}
else{
printf("Wrong code");
}

return 0;
}
___________________________________________________________________
switch statement-1 #33

#include <stdio.h>
int main(void){
int day_number;
printf("Enter day number:");
scanf("%d\n",& day_number);
switch(day_number){
case 0:
printf("Saturday\n");
break;

 case 1:
printf("sunday\n");
break;

 case 2:
printf("monday\n");
break;

 case 3:
printf("tuesday\n");
break;

 case 4:
printf("wednesday\n");
break;

 case 5:
printf("thursday\n");
break;

 case 6:
printf("friday\n");
break;
default:
printf("Wrong day number\n");
}
return 0;
}
