leap year.c #41

#include <stdio.h>
int main(){
int year;
printf("Enter year-");
scanf("%d",&year);
int isleapyear =0;

if(((year%4==0)&&(year%100!=0))||(year%400==0)){
isleapyear =1;
}
if(isleapyear){
printf("%d is a leap year\n",year);

}else{
printf("%d is not a leap year",year);
}
return 0;
}
